{
  "name": "transaction",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "export ENV_ID=dev && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "export ENV_ID=test && jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-fastify": "^6.8.3",
    "@types/mathjs": "^6.0.2",
    "azure-keyvault": "^3.0.5",
    "bchaddrjs": "^0.4.4",
    "big.js": "^5.2.2",
    "bignumber.js": "^9.0.0",
    "bitcoinjs-lib": "^5.1.6",
    "bn.js": "^5.0.0",
    "bs58check": "^2.1.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-tx-keyvault": "^1.0.6",
    "ethereumjs-util": "^6.2.0",
    "fs": "0.0.1-security",
    "mathjs": "^6.2.5",
    "ms-rest-azure": "2.6.0",
    "nestjs": "0.0.1",
    "nock": "^11.7.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "satoshi-bitcoin-ts": "^0.2.4",
    "secp256k1": "^3.7.1",
    "web3": "^1.2.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.10.5",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/bchaddrjs": "^0.4.0",
    "@types/big.js": "^4.0.5",
    "@types/bignumber.js": "^5.0.0",
    "@types/bn.js": "^4.11.5",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typechain": "^1.0.3",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
